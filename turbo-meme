class AdvancedCombineViewModel: ObservableObject {
    
    @Published var data: [String] = []
    @Published var error = ""
    let dataService = AdvancedCombineDataService()
    
    var cancellables = Set<AnyCancellable>()
    
    init() {
        addSubscribers()
    }
